{
  "name": "jura",
  "version": "0.1.0",
  "private": true,
  "description": "team project WCS",
  "author": "loicbdev and nicholas570",
  "license": "ISC",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:watch": "react-scripts test --coverage --runInBand --watchAll",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .ts --ext .tsx --fix",
    "lint:check": "eslint .",
    "prettier:write": "prettier --write **/*.{ts,tsx,json} && prettier --write *.{ts,tsx,json}",
    "precommit": "lint-staged",
    "generate": "graphql-codegen --config codegen.yml",
    "playwright": "playwright test",
    "playwright-debug-windows": "set PWDEBUG=1 && npx playwright test"
  },
  "dependencies": {
    "@apollo/client": "^3.5.5",
    "@graphql-codegen/cli": "^4.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/bootstrap": "^5.1.6",
    "@types/date-fns": "^2.6.0",
    "@types/history": "^4.7.9",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/node-sass": "^4.11.2",
    "@types/react": "^17.0.0",
    "@types/react-bootstrap": "^0.32.29",
    "@types/react-dom": "^17.0.0",
    "@types/react-icons": "^3.0.0",
    "@types/react-router-dom": "^5.3.2",
    "bootstrap": "^5.1.3",
    "chart.js": "^3.7.1",
    "date-fns": "^2.28.0",
    "graphql": "^15.3.0",
    "history": "^5.1.0",
    "lint-staged": "^11.1.2",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.1",
    "react-chartjs-2": "^4.1.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.0",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "4.0.0",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-operations": "2.2.1",
    "@graphql-codegen/typescript-react-apollo": "3.2.2",
    "@playwright/test": "^1.25.1",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/graphql": "^14.5.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "apollo": "^2.33.9",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "react-error-overlay": "6.0.9"
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint",
      "npm run prettier:write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/App.tsx",
      "!src/components/router/Router.tsx",
      "!src/reportWebVitals.ts",
      "!src/setupTests.ts",
      "!src/services/api/generated/graphql.ts"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 90,
        "functions": 90
      }
    }
  }
}
